local solarSystem, solarSystemDep = asset.importToggle("solarsystem", true)

solarSystemDep.onInitialize = function ()
    openspace.setPropertyValue("SunMarker.renderable.enabled", false)
    openspace.setPropertyValue("EarthMarker.renderable.enabled", false)
    openspace.setPropertyValue("Constellation Bounds.renderable.enabled", false)
    openspace.setPropertyValue("MilkyWay.renderable.transparency", 0.55)
    openspace.setPropertyValue("MilkyWay.renderable.segments", 50)

    openspace.time.setTime(openspace.time.currentWallTime())
    dofile(openspace.absPath('${SCRIPTS}/bind_common_keys.lua'))
    openspace.resetCameraDirection()
    openspace.addVirtualProperty("BoolProperty", "Show Trails", "*Trail.renderable.enabled", true, nil, nil)

end

solarSystemDep.onDeinitialize = function ()
    openspace.removeVirtualProperty("*Trail.renderable.enabled")
end

asset.onInitialize = function () 
   -- Do nothing. 
end