local assetHelper = asset.require('util/asset_helper')
local sharedSssb = asset.require('./sssb_shared')

local filepath = sharedSssb.downloadSssbDatabaseFile(asset, 'main asteroid belt', 'sssb_data_main_belt_asteroid')
local object = sharedSssb.createSssbGroupObject("sssb_data_main_belt_asteroid.csv", filepath, { 0.9, 0.3, 0.1 })
object.Renderable.Enabled = false
object.Renderable.Segments = 40
object.Renderable.Fade = 3
object.Renderable.UpperLimit = 50000

assetHelper.registerSceneGraphNodesAndExport(asset, { object })
